{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": true,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "./dist/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/[locale]/components/AddButton/AddButton.tsx", "src/appsrc/app/[locale]/posts/page.tsx/app/[locale]/components/LogOutButton/LogOut.jsx", "src/app/api/auth/[auth0]/route.js", "tailwind.config.js", "postcss.config.js"  ],
  "exclude": [
    "node_modules"
  ]
}
